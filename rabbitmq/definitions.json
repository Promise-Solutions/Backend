{
  "vhosts": [
    { "name": "/" }
  ],
  "users": [
    {
      "name": "studiozero-admin",
      "password": "studiozero-rmq-pass",
      "tags": "administrator"
    }
  ],
  "permissions": [
    {
      "user": "studiozero-admin",
      "vhost": "/",
      "configure": ".*",
